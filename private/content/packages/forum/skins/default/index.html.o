    <script type="text/javascript">
      $('.textCollapse').each(function(){
        var html = $(this).html();
        // alert(html);
        $(this).attr('full_text', html);
        $(this).html(html.substring(0, 170) + ' ...');
        $(this).css('display', 'block');
        $(this).show().animate({opacity:1});
        $(this).css('cursor', 'pointer');
      });
      $('.textCollapse').click(function(){
        var thisId = this;
        if($(thisId).attr('ignorMy')) {
          $(thisId).removeAttr('ignorMy');
          return;
        }
        // alert('123');
        var html = $(thisId).attr('full_text');
        if(html) {
          var randId = Math.floor(Math.random() * 100000) + 1;
          $(thisId).css('opacity', '0');
          $(thisId).html(html + '<p>&nbsp;</p><p align="right"><button class="btn btn_default myButton" id="' + randId + '"><span class="glyphicon glyphicon-menu-up" aria-hidden="true"></span></button></p>');
          $(thisId).removeAttr('full_text');
          $(thisId).css('cursor', 'default');
          $(thisId).show().animate({opacity:1},1000);
          $('#' + randId).click(function(){
            $(thisId).css('opacity', '0');
            $(thisId).html(html.substring(0, 170) + ' ...');
            $(thisId).attr('full_text', html);
            $(thisId).attr('ignorMy', '1');
            $(thisId).css('cursor', 'pointer');
            $(thisId).animate({opacity:1},1000);
            return;
          });
        }
      });
    </script>
